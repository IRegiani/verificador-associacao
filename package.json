{
  "name": "verificador-associacao",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "clean": "cross-env rm -rf ./coverage && cross-env rm -rf ./node_modules",
    "local": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --config .eslintrc.js --ext .jsx,.js ./src",
    "test": "npm run lint && jest --config ./config/jest.config.js --coverage",
    "test:no-cov": "npm run lint && jest --config ./config/jest.config.js",
    "test:verbose": "npm run lint && jest --config ./config/jest.config-verbose.js",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "4.9.12",
    "@material-ui/icons": "^4.9.1",
    "cross-env": "7.0.2",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "jest": "25.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.4.4",
    "react-styleguidist": "11.0.5",
    "source-map-explorer": "2.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
